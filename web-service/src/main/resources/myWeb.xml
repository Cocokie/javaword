<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>lol</servlet-name>
        <servlet-class>com.lix.servlet.LolServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pop</servlet-name>
        <servlet-class>com.lix.servlet.PopServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>lol</servlet-name>
        <url-pattern>/lo</url-pattern>
        <url-pattern>/l</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pop</servlet-name>
        <url-pattern>/po</url-pattern>
        <url-pattern>/p</url-pattern>
    </servlet-mapping>
</web-app>